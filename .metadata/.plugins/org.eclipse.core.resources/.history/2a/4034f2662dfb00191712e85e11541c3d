
public class ReferenceCastingTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	
	static Object getStudent()
	{
		
		Student s = new Student (111, "abc");
		return s;
	}

}

class Student
{
	
	int rollno;
	String name;
	
	Student(int r, String n){
		
		rollno = r;
		name = n;
		
	}
	
	int getRollNo()
	{
		
		return rollno;
	}
	
	String getName()
	{
		
		return name;
	}
}
